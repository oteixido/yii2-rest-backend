{
    "name": "oteixido/yii2-rest-backend",
    "description": "Components to use a REST backend in Yii2 applications",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Oriol Teixid√≥",
            "email": "oriol.teixido@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "codeception/codeception": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "oteixido\\rest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "oteixido\\rest\\": "tests/"
        }
    }
}
